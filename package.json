{
  "name": "@yababay67/svelte-meets-pug",
  "version": "0.1.4",
  "description": "Reusable pug components for svelte modules.",
  "main": "index.js",
  "scripts": {
    "copy"        : "node -r '@yababay67/svelte-meets-pug' -e 'const copy = require(\"@yababay67/svelte-meets-pug\"); copy()'",
    "html"        : "rollup --config ./src/pug_modules/scripts/rollup.html.js",
    "settings"    : "rollup --config ./src/pug_modules/scripts/rollup.settings.js",
    "index"       : "node ./src/pug_modules/scripts/index.js",
    "postinstall" : "npm copy && npm html && npm settings && npm index",

    "clean" : "rm -rf docs/index.html src/pug_modules scripts/html.js scripts/settings.js",
    "test"  : "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yababay/svelte-meets-pug.git"
  },
  "keywords": [
    "svelte",
    "web",
    "template",
    "pug"
  ],
  "author": "Michael Belyakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yababay/svelte-meets-pug/issues"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "recursive-copy": "^2.0.13",
    "rollup-plugin-pug": "^1.1.1"
  },
  "homepage": "https://github.com/yababay/svelte-meets-pug#readme",
  "dependencies": {
    "@yababay67/svelte-meets-pug": "^0.1.3"
  }
}
